source ~/.vimrc

syntax on
set paste
set columns=136
set lines=50


set guifont=Monospace\ 11

"---------------------------------------------
" ENCODING SETTINGS
"---------------------------------------------
set encoding=utf-8
set termencoding=utf-8
set fileencoding=utf-8
set fileencodings=utf-8,ucs-bom,cp936,gb18030,big5,euc-jp,euc-kr,latin1
set fileformat=unix

